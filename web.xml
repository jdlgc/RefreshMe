<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>addWebPage</servlet-name>
    <jsp-file>/WEB-INF/views/addWebPage.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>configureProfile</servlet-name>
    <jsp-file>/WEB-INF/views/configureProfile.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>logIn</servlet-name>
    <jsp-file>/WEB-INF/views/logIn.jsp</jsp-file>
  </servlet>
    <servlet>
    <servlet-name>webPages</servlet-name>
    <jsp-file>/WEB-INF/views/webPages.jsp</jsp-file>
  </servlet>
      <servlet>
    <servlet-name>webPagesAdministrator</servlet-name>
    <jsp-file>/WEB-INF/views/webPagesAdministrator.jsp</jsp-file>
  </servlet>

  <servlet-mapping>
    <servlet-name>addWebPage</servlet-name>
    <url-pattern>/addWebPage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>configureProfile</servlet-name>
    <url-pattern>/configureProfile</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>logIn</servlet-name>
    <url-pattern>/logIn</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>webPages</servlet-name>
    <url-pattern>/webPages</url-pattern>
  </servlet-mapping>
      <servlet-mapping>
    <servlet-name>webPagesAdministrator</servlet-name>
    <url-pattern>/webPagesAdministrator</url-pattern>
  </servlet-mapping>

</web-app>
